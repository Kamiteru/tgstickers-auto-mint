# STICKERDOM API
STICKERDOM_JWT_TOKEN=your_jwt_token_here

# PAYMENT METHODS
PAYMENT_METHODS=TON,STARS

# TON WALLET (WalletV5R1, min 0.1 TON)
TON_SEED_PHRASE=your_ton_wallet_seed_phrase_here
TON_ENDPOINT=mainnet

# TELEGRAM STARS PAYMENT (via Telethon)
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here
TELEGRAM_PHONE=+your_phone_number_here
TELEGRAM_SESSION_NAME=stars_payment_session

# STARS ADVANCED CONFIGURATION
STARS_MAX_PURCHASES_PER_SESSION=3
STARS_PURCHASE_INTERVAL=2.0
STARS_SESSION_COOLDOWN=30
STARS_MAX_RETRY_ATTEMPTS=3
STARS_ADAPTIVE_LIMITS=true
STARS_CONCURRENT_PURCHASES=false
STARS_PAYMENT_TIMEOUT=120
STARS_INVOICE_TIMEOUT=30
STARS_PROFILE=balanced  # conservative/balanced/aggressive/extreme

# CAPTCHA SOLVING
CAPTCHA_ENABLED=true
ANTICAPTCHA_API_KEY=your_anticaptcha_api_key_here
CAPTCHA_TIMEOUT=300

# RATE LIMITER
RATE_LIMITER_ENABLED=true
RATE_LIMITER_DB_PATH=data/rate_limiter.db

# Rate Limiter Profile (safe/balanced/fast/aggressive/extreme)
# Can also be set via command line: --fast, --aggressive, etc.
RATE_LIMITER_PROFILE=balanced

# Manual override settings (optional, profile settings take precedence)
RATE_LIMITER_MAX_DELAY=300
RATE_LIMITER_CIRCUIT_BREAKER_THRESHOLD=3
RATE_LIMITER_CIRCUIT_BREAKER_TIMEOUT=300
RATE_LIMITER_PREEMPTIVE_DELAY=60
RATE_LIMITER_AGGRESSIVE_BACKOFF_MULTIPLIER=2.0

# NOTIFICATIONS (separate bot for status updates)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# PROXY SETTINGS (for IP ban bypass)
PROXY_ENABLED=false
PROXY_URL=
# Examples:
# PROXY_URL=http://username:password@proxy.example.com:8080
# PROXY_URL=socks5://proxy.example.com:1080
# PROXY_URL=http://127.0.0.1:8080

# USER AGENT ROTATION (for IP ban bypass)
USER_AGENT_ROTATION=true

# API SETTINGS
REQUEST_TIMEOUT=30
MAX_RETRIES_PER_REQUEST=5
PRICE_CACHE_TTL=30
COLLECTION_CHECK_INTERVAL=5
COLLECTION_NOT_FOUND_RETRY=30

# LOGGING
LOG_LEVEL=INFO
LOG_TO_FILE=true
LOG_FILE_PATH=logs/sticker_bot.log

# OPERATION MODES
DRY_RUN_MODE=false
TEST_MODE=false
MONITORING_INTERVAL=1

# ENDPOINT DISCOVERY SETTINGS (optional)
SELENIUM_HEADLESS_DISABLED=false  # Set to True for debugging browser
ENDPOINT_VALIDATION_ENABLED=true  # Enable adaptive endpoint system
ENDPOINT_VALIDATION_TIMEOUT=2.0   # Max time for endpoint validation (seconds)
