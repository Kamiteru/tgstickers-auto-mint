{
  "title": "TG Stickers Auto Mint",
  "description": "Автоматическая покупка стикеров через Telegram Stars и TON",
  "author": "TG Stickers Auto Mint Team",
  "language": "ru",
  "gitbook": ">=3.0.0",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-default",
    "search",
    "lunr",
    "sharing"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "sharing": {
      "facebook": false,
      "twitter": false,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["telegram"]
    }
  },
  "links": {
    "sidebar": {
      "GitHub": "https://github.com/yourusername/tgstickers-auto-mint"
    }
  }
} 